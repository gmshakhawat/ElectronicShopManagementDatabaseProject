<AnchorPane id="AnchorPane" fx:id="AnchorPane" prefHeight="550.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="dbms.RemoveItemFXMLDocumentController">
   <children>
      <ImageView pickOnBounds="true" preserveRatio="true">
