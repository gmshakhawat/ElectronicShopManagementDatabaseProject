            <JFXButton fx:id="search" buttonType="RAISED" layoutX="463.0" layoutY="27.0" onAction="#button1" prefHeight="25.0" prefWidth="104.0" style="-fx-background-color: #560c59;" text="Search" textFill="WHITE" />
            <JFXButton fx:id="refresh" layoutX="11.0" layoutY="332.0" onAction="#button1" prefHeight="25.0" prefWidth="104.0" style="-fx-background-color: #560c59;" text="Refresh" textFill="WHITE" />
